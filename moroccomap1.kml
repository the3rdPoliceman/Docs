<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Morocco Road Trip Itinerary</name>
    <description>7-day road trip starting from Marrakech</description>

    <!-- Day 1 -->
    <Placemark>
      <name>Day 1: Marrakech to Ouarzazate</name>
      <description>Drive from Marrakech to Ouarzazate via Tizi n'Tichka Pass and visit Ait Ben Haddou.</description>
      <Point>
        <coordinates>-7.9811,31.6295,0</coordinates> <!-- Marrakech -->
      </Point>
    </Placemark>
    <Placemark>
      <name>Ait Ben Haddou</name>
      <description>UNESCO World Heritage site.</description>
      <Point>
        <coordinates>-7.1312,31.0470,0</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>Ouarzazate</name>
      <description>Known as the Gateway to the Sahara.</description>
      <Point>
        <coordinates>-6.9094,30.9180,0</coordinates>
      </Point>
    </Placemark>

    <!-- Day 2 -->
    <Placemark>
      <name>Day 2: Ouarzazate to Todra Valley</name>
      <description>Drive from Ouarzazate to Todra Valley, visit Skoura Oasis and Kasbah Amridil.</description>
      <Point>
        <coordinates>-5.9993,31.5085,0</coordinates> <!-- Todra Valley -->
      </Point>
    </Placemark>
    <Placemark>
      <name>Skoura Oasis</name>
      <description>Known for its palm groves and Kasbah Amridil.</description>
      <Point>
        <coordinates>-6.5326,31.0510,0</coordinates>
      </Point>
    </Placemark>

    <!-- Day 3 -->
    <Placemark>
      <name>Day 3: Todra Valley to Merzouga</name>
      <description>Drive from Todra Valley to Merzouga, visit Erg Chebbi dunes.</description>
      <Point>
        <coordinates>-4.0105,31.1506,0</coordinates> <!-- Merzouga -->
      </Point>
    </Placemark>

    <!-- Day 4 -->
    <Placemark>
      <name>Day 4: Merzouga to Erfoud and Rissani</name>
      <description>Explore the desert region and visit Erfoud and Rissani.</description>
      <Point>
        <coordinates>-3.9079,31.4333,0</coordinates> <!-- Erfoud -->
      </Point>
    </Placemark>
    <Placemark>
      <name>Rissani</name>
      <description>Visit the traditional souk and historical sites.</description>
      <Point>
        <coordinates>-4.0127,31.2820,0</coordinates>
      </Point>
    </Placemark>

    <!-- Day 5 -->
    <Placemark>
      <name>Day 5: Erfoud to Draa Valley</name>
      <description>Drive from Erfoud to Draa Valley, explore palm groves and kasbahs.</description>
      <Point>
        <coordinates>-6.2931,30.7624,0</coordinates> <!-- Draa Valley -->
      </Point>
    </Placemark>

    <!-- Day 6 -->
    <Placemark>
      <name>Day 6: Draa Valley to Zagora</name>
      <description>Continue to Zagora, visit Tamegroute library and pottery workshops.</description>
      <Point>
        <coordinates>-5.8384,30.3328,0</coordinates> <!-- Zagora -->
      </Point>
    </Placemark>

    <!-- Day 7 -->
    <Placemark>
      <name>Day 7: Zagora to Marrakech</name>
      <description>Drive back to Marrakech via the High Atlas Mountains.</description>
      <Point>
        <coordinates>-7.9811,31.6295,0</coordinates> <!-- Marrakech -->
      </Point>
    </Placemark>

  </Document>
</kml>
